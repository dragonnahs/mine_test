@import '../../styles/index.styl';
body,html
  size(100%, 100%)
  background: #000
  #appbox
    width: 100vw
    min-height: 100vh
    box-sizing: border-box
    background-image: url('../../assets/tu_starplan/sign_painterbg.png')
    background-size: 100%
    background-repeat: no-repeat
    padding: 16px 32px 48px
    .sign_topbox
      height: 119px
      text-align: center
      .logo_box
        width: 269px
        height: 119px
        object-fit: contain
    .list_box
      list-style: none
      margin: 21px 0 0
      .item_box
        margin-bottom: 30px
        .top_log
          width: 227px
          height: 46px
          margin-left: 41px
          margin-bottom: 20px
        .list_tip
          font-size: 14px
          font-family: PingFangSC-Regular, PingFang SC
          font-weight: 400
          color: #FFFFFF
          line-height: 22px
    .okbtn
      width: 243px
      height: 45px
      margin-left: 33px
      object-fit: contain
    .tip_box
      display: flex
      justify-content: center
      align-items: center
      margin-top: 14px
      .checked_img
        width: 16px
        height: 17px
      .commtext
        font-size: 14px
        font-family: PingFangSC-Regular, PingFang SC
        font-weight: 400
        color: #FFFFFF
      .tip_text
        margin-left: 7px
      .tip_protocal
        color: #00AAFF

    